        else if (ColumnProperty == "Role") {
            var parameter = { TableId: tableId, SystemField: " and IsSystem = 0", Sort: "SlNo" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetRole", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setrole').html(result);
                }
            });
        }
        else if (ColumnProperty == "FieldValidation") {
            var parameter = { TableId: tableId, SystemField: " and IsSystem = 0", Sort:"SlNo"}
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetFieldValidation", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setfieldvalidation').html(result);
                }
            });
        }
    }

    $("#FilterCondition").change(function () {

        var sc = $("#SortingCondition").val();
        var ColumnProperty = $("#SaveSetColumnProperty").val();
        var tableid = $("#tblid").val();

        var fc = $("#FilterCondition").val();
        if (fc == "FDDMLC") {
            fc = " and ColumnName = (select ColumnName from Utl_Dynamic_Table_Column where TableId = " + tableid + " group by ColumnName having COUNT(columnName) > 1)";
        }
        else if (fc == "FDVC") {
            fc = " and SelectColumnName = (select SelectColumnName from Utl_Dynamic_Table_Column where TableId = " + tableid + " group by SelectColumnName having COUNT(SelectColumnName) > 1)";
        }
        else if (fc == "FDFC") {
            fc = " and ColumnDisplayName = (select ColumnDisplayName from Utl_Dynamic_Table_Column where TableId = " + tableid + " group by ColumnDisplayName having COUNT(ColumnDisplayName) > 1)";
        }

        if (ColumnProperty == "DF") {
            var parameter = { TableId: tableid, Filter:fc, Sort: sc }
            $.ajax({
                type: "POST",
                url: '@Url.Action("Setup", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setup').html(result);

                }

            });
        }

    });

    $("#SortingCondition").change(function () {


        var sc = $("#SortingCondition").val();
        var fc = $("#FilterCondition").val();
        var ColumnProperty = $("#SaveSetColumnProperty").val();
        var tableid = $("#tblid").val();

        var systemField = $('#systemFieldFilter').val();
        //alert("Handler for .change() called. ColumnProperty: " + ColumnProperty + " filterconditon: " + fc + tableid);

        if (ColumnProperty == "DF") {

            var fc = $("#FilterCondition").val();
            if (fc == "FDDMLC") {
                fc = " and ColumnName = (select ColumnName from Utl_Dynamic_Table_Column where TableId = " + tableid + " group by ColumnName having COUNT(columnName) > 1)";
            }
            else if (fc == "FDVC") {
                fc = " and SelectColumnName = (select SelectColumnName from Utl_Dynamic_Table_Column where TableId = " + tableid + " group by SelectColumnName having COUNT(SelectColumnName) > 1)";
            }
            else if (fc == "FDFC") {
                fc = " and ColumnDisplayName = (select ColumnDisplayName from Utl_Dynamic_Table_Column where TableId = " + tableid + " group by ColumnDisplayName having COUNT(ColumnDisplayName) > 1)";
            } else if (fc == "") {
                fc = systemField;
            }

            var parameter = { TableId: tableid, Filter: fc, Sort: sc }
            $.ajax({
                type: "POST",
                url: '@Url.Action("Setup", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setup').html(result);

                }

            });
        }
        else if (ColumnProperty == "Layouts") {
            var parameter = { TableId: tableid, Sort: sc }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetLayout", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setlayout').html(result);
                }
            });
        }
        else if (ColumnProperty == "Variable") {
