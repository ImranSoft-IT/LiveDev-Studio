<script>

    function Unmap(tableId, tableName) {

        //$('#SaveSetColumnProperty').val('');
        //$('#SaveSetColumnProperty').val('unmap');

        var parameter = { TableId: tableId, TableName: tableName}
        $.ajax({
            type: "POST",
            url: '@Url.Action("ShowUnmapColumn", "Page")',
            dataType: "html",
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify(parameter),
            success: function (result) {
                $('#setup').html(result);

            }
        });
    }

    function SystemFd(tableId) {
        var ColumnProperty = $("#SaveSetColumnProperty").val();
        $('#systemFieldFilter').val(' and IsSystem = 1 ');

        if (ColumnProperty == "DF") {
            var parameter = { TableId: tableId, Filter: " and IsSystem = 1 ", Sort: "SlNo" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("Setup", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setup').html(result);
                }
            });
        }
        else if (ColumnProperty == "DML") {
            var parameter = { TableId: tableId, IsSyatem: " and IsSystem = 1" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetDML", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setdml').html(result);
                }
            });
        }
        else if (ColumnProperty == "Variable") {
            var parameter = { TableId: tableId, SystemField: " and IsSystem = 1", Sort: 'SlNo' }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetVariable", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setvariable').html(result);
                }
            });
        }
        else if (ColumnProperty == "HyperLink") {
            var parameter = { TableId: tableId, SystemField: " and IsSystem = 1", Sort: "SlNo" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetHyperLink", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#sethyperlink').html(result);
                }
            });
        }
        else if (ColumnProperty == "Role") {
            var parameter = { TableId: tableId, SystemField: " and IsSystem = 1", Sort: "SlNo" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetRole", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setrole').html(result);
                }
            });
        }
        else if (ColumnProperty == "FieldValidation") {
            var parameter = { TableId: tableId, SystemField: " and IsSystem = 1", Sort:"SlNo"}
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetFieldValidation", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setfieldvalidation').html(result);
                }
            });
        }
    }

    function DefaultColumnProperty(tableId) {
        var ColumnProperty = $("#SaveSetColumnProperty").val();

        if (ColumnProperty == "DF") {
            var parameter = { TableId: tableId, Filter: " and IsSystem = 0 ", Sort: "SlNo" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("Setup", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setup').html(result);
                }
            });
        }
        else if (ColumnProperty == "DML") {
            var parameter = { TableId: tableId, IsSyatem: " and IsSystem = 0" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetDML", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setdml').html(result);
                }
            });
        }
        else if (ColumnProperty == "Layouts") {
            var parameter = { TableId: tableId, Sort: "SlNo asc" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetLayout", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setlayout').html(result);
                }
            });
        }
        else if (ColumnProperty == "Variable") {
            var parameter = { TableId: tableId, SystemField: " and IsSystem = 0", Sort: 'SlNo' }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetVariable", "Page")',
                dataType: "html",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(parameter),
                success: function (result) {
                    $('#setvariable').html(result);
                }
            });
        }
        else if (ColumnProperty == "HyperLink") {
            var parameter = { TableId: tableId, SystemField: " and IsSystem = 0", Sort: "SlNo" }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SetHyperLink", "Page")',
